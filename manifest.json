{
	"manifest_version": 2,
	"name": "New PayPal Ext By Nika Gorgisheli",
	"version": "0.1",
	"permissions": [ "tabs" ],
	"content_scripts": [
		{
			"matches": [ "*://*.paypal.com/*" ],
			"js": [ "config.js", "content.js" ]
		}
	],

	"background": {
		"scripts": [ "background.js" ]
	},
	"browser_action": {
		"default_icon": "icon.png"
	}
}
